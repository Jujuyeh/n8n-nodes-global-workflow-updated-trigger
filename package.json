  "author": "Jujuyeh",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jujuyeh/n8n-nodes-global-workflow-updated-trigger.git"
  },
  "bugs": {
    "url": "https://github.com/Jujuyeh/n8n-nodes-global-workflow-updated-trigger/issues"
  },
  "homepage": "https://github.com/Jujuyeh/n8n-nodes-global-workflow-updated-trigger#readme",
  "main": "dist/nodes/GlobalWorkflowUpdatedTrigger.node.js",
  "types": "dist/nodes/GlobalWorkflowUpdatedTrigger.node.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
